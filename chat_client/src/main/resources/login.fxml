      <Label layoutX="26.0" layoutY="43.0" text="Логин:" />
      <Label layoutX="20.0" layoutY="82.0" text="Пароль:" />
      <TextField fx:id="loginText" layoutX="73.0" layoutY="39.0" prefHeight="25.0" prefWidth="190.0" />
      <PasswordField fx:id="passText" focusTraversable="false" layoutX="73.0" layoutY="78.0" prefHeight="25.0" prefWidth="190.0" />
      <Label fx:id="warningMes" focusTraversable="false" layoutX="16.0" layoutY="5.0" text="Не верный логин или пароль!" textFill="#fd1c08" visible="false">
